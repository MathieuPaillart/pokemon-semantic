@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@base <http://www.polytech.fr/pokemon#> .

######################   CLASS   ############################
<Animal> a owl:Class;
  rdfs:label "animal"@en, "animal"@fr ;
  rdfs:comment "a living organism characterized by voluntary movement."@en, "être vivant doué de sensibilite de mobilité."@fr .

<Monsters> a owl:Class;
    rdfs:label "monsters"@en, "monstres"@fr ;
  rdfs:comment "A monster is often a type of grotesque creature, whose appearance frightens and whose powers of destruction threaten the human world's social or moral order."@en, "est un individu ou une créature dont l'apparence, voire le comportement, surprend par son écart avec les normes d'une société."@fr .

<Event>
 a owl:Class.

<Match>
  a owl:Class;
  rdfs:subClassOf <Event>;
  owl:equivalentClass [a owl:Restriction; owl:onProperty <playingTeam>;owl:maxCardinality <Team>];
  rdfs:label "match"@en, "rencontre"@fr ;
  rdfs:comment "a match between two teams."@en, "une rencontre entre deux équipes."@fr .

<SportCategory>
  a owl:Class;
  rdfs:label "sport category"@en, "catégorie de sport"@fr;
  rdfs:comment "can be football, basketball, etc."@en, "peut être football, basketball etc"@fr.

<Team>
  a owl:Class;
  rdfs:label "team"@en, "équipe"@fr;
  rdfs:comment"is a group of individuals working together to achieve their goal."@en,"est un petit groupe d'individus partenaires dans un but commun."@fr.
<WomanTeam>
  a owl:Class;
  rdfs:subClassOf <Team>,[a owl:Restriction; owl:onProperty <hasPlayer>; owl:allValuesFrom <Woman>];
  rdfs:label "woman team"@en, "équipe féminine"@fr;
  rdfs:comment "at least one woman in the team and no man"@en, "au moins une femme dans l'équipe et pas de garçon"@fr.
<ManTeam>
  a owl:Class;
  rdfs:subClassOf <Team>,[a owl:Restriction; owl:onProperty <hasPlayer>; owl:allValuesFrom <Man>];
  rdfs:label "man team"@en, "équipe masculine"@fr;
  rdfs:comment "at least one man in the team and no woman"@en, "au moins un garçon dans l'équipe et pas de fille".

<Male>
  a owl:Class ;
  owl:disjointWith <Female> ;
  rdfs:label "male"@en, "mâle"@fr ;
  rdfs:comment "an animal that produces gametes (spermatozoa) that can fertilize female gametes (ova)."@en, "individu appartenant au sexe qui possède le pouvoir de fécondation."@fr .

<Female>
  a owl:Class;
  rdfs:label "female"@en, "femelle"@fr ;
  rdfs:comment " an animal that produces gametes (ova) that can be fertilized by male gametes (spermatozoa)."@en, "animal appartenant au sexe apte à produire des ovules."@fr .

<Man>
  a owl:Class;
  rdfs:subClassOf <Person>, <Male>;
  rdfs:label "man"@en, "homme"@fr ;
  rdfs:comment "an adult male person"@en, "mâle adulte de l'espèce humaine."@fr .

<Woman>
  a owl:Class ;
  rdfs:subClassOf <Person>, <Female>;
  rdfs:label "woman"@en, "femme"@fr ;
  rdfs:comment "an adult female person"@en, "femelle adulte de l'espèce humaine."@fr .

<Person>
  a owl:Class;
  rdfs:label "person"@en, "human being"@en, "human"@en, "personne"@fr, "être humain"@fr, "humain"@fr, "homme"@fr ;
  rdfs:comment "a member of the human species"@en, "un membre de l'espèce humaine."@fr .

<Player>
  a owl:Class;
  rdfs:subClassOf <Person>;
  rdfs:label "player"@en, "joueur"@fr ;
  rdfs:comment "a person which plays a sport"@en, "une personne qui joue d'un sport"@fr .

<ManPlayer>
 a owl:Class;
 owl:intersectionOf ( <Player> <Man> ) .

<WomanPlayer>
a owl:Class;
owl:intersectionOf ( <Player> <Woman> ) .
######################   ObjectProperty   ############################

<hasPlayer>
  a owl:ObjectProperty;
  rdfs:domain <Team>;
  rdfs:range <Player>;
  rdfs:label "has player"@en, "a un joueur"@fr ;
  rdfs:comment "a player in a team."@en, "un joueur d'une équipe."@fr .

<playingTeam>
 a owl:ObjectProperty;
  rdfs:domain <Event> ;
  rdfs:range <Team> ;
  rdfs:label "has team"@en, "a une équipe"@fr ;
  rdfs:comment "a team being part of a match."@en, "Une équipe qui fait partie d'un match"@fr .

<winningTeam>
  a owl:ObjectProperty;
  rdfs:domain <Event>;
  rdfs:range <Team>;
  rdfs:label "winning team"@en, "équipe gagnante"@fr ;
  rdfs:comment "the winning team of an event"@en, "l'équipe gagnante d'un événement"@fr .

<sportType>
a owl:ObjectProperty;
  rdfs:domain <Event>;
  rdfs:range <SportCategory>;
  rdfs:label "type"@en, "type"@fr ;
  rdfs:comment "type of sport the team plays"@en, "type de sport joué par l'équipe"@fr .

<rivalTeam>
a owl:ObjectProperty, owl:SymmetricProperty;
  rdfs:domain <Team>;
  rdfs:range <Team>;
  rdfs:label "rival team"@en, "équipe rivale"@fr.


######################   DatatypeProperty   ############################
<eventTitle>
  a owl:DatatypeProperty ;
  rdfs:domain <Event>;
  rdfs:label "eventTitle"@en, "titre de l'événement"@fr.
<size>
  a owl:DatatypeProperty;
  rdfs:domain <Team>;
  rdfs:label "size"@en, "taille"@fr ;
  rdfs:comment "max size of a team"@en, "taille maximum de l'équipe"@fr .

<teamName>
 a owl:DatatypeProperty;
  rdfs:domain <Team>;
  rdfs:label "name"@en, "nom"@fr ;
  rdfs:comment "nickname of a team"@en, "pseudo de l'équipe"@fr .


<PersonName>
 a owl:DatatypeProperty;
  rdfs:domain <Person>;
  rdfs:label "name"@en, "nom"@fr ;
  rdfs:comment "person's name"@en, "nom d'une personne"@fr .

<age>
  a owl:DatatypeProperty ;
  rdfs:domain <Person>;
  rdfs:label "age"@en, "âge"@fr ;
  rdfs:comment "complete existence duration."@en, "durée complète d'existence."@fr .
